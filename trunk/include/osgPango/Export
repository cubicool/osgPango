    #    define OSGPANGO_EXPORT   __declspec(dllexport)
    #  else
    #    define OSGPANGO_EXPORT   __declspec(dllimport)
    #  endif
#else
    #  define OSGPANGO_EXPORT
#endif  

#endif  // OSGCAIRO_EXPORT
